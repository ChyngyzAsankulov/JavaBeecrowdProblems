import java.io.IOException;
 import java.util.Scanner;
 
 public class Main {
    public static void main(String[] args) throws IOException {
       Scanner input = new Scanner(System.in);
      int A,C,B,D,R;
      A=input.nextInt();
      B=input.nextInt();
      C=input.nextInt();
      D=input.nextInt();
      R=A * B - C * D;
      System.out.print("DIFERENCA = "+R+"\n");
    }
}
